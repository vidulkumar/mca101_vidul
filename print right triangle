
def rtTriangle():
      '''
      objective: To print right triangle
      approach : To print astriks line by line
      '''
      print ('*')
      print ('**')
      print ('***')
      print ('****')
    
def main():
    '''
        objective : To print right triangle
        approach  : by calling rtTriangle function
    '''
    rtTriangle()
    print ('End of output')

if __name__ == '__main__':
      main()
print('End of program')
